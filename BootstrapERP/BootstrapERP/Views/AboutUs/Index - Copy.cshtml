
@{
    ViewBag.Title = "About";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="page-header">
                <h4 style="display:inline-block"><span class="hapText"> @ViewBag.Title </span><small> </small></h4>
                <ol class="breadcrumbLine" style="float:right;display:inline-block">
                    <li class="hapText">@Html.ActionLink("Home", "Index", "BlogPosts")</li>
                    <li class="hapText">@ViewBag.Title</li>
                </ol>
            </div>
        </div>
    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-md-12" style="text-align:center">
            <div class="col-sm-12">
                <section style="text-align:center" id="module-anchor-statement-mission-statement" data-section-id="mission-statement" class="module-anchor-statement center module-anchor-statement__header-one">
                    <div>
                        <h3>
                            <span class="green">About AllTimeStudy</span>
                        </h3>
                        <h3>
                            AllTimeStudy is a modern learning and career growth platform
                            <span class="blue">designed to help students and professionals</span>
                            reach their full potential.
                            <span class="red">We provide reliable, easy-to-understand, and industry-relevant resources</span>
                            that make learning smarter,
                            <span class="green">skills stronger,</span>
                            and
                            <span class="yellow">careers more successful</span>.
                        </h3>
                    </div>
                </section>
            </div>

        </div>

    </div>
</div>

<div class="container">
    <div class="row">
        <div class="col-md-12" style="text-align:center">
            <div class="col-sm-12">
                <section style="text-align:center" id="module-anchor-statement-mission-statement" data-section-id="mission-statement" class="module-anchor-statement center module-anchor-statement__header-one">
                    <div>
                        <h3>
                            <span class="blue">Our Mission</span>
                        </h3>
                        <h3>
                            Our mission is to
                            <span class="blue">empower learners worldwide</span>
                            with accessible, high-quality, and practical education.
                            <span class="red">AllTimeStudy helps students, professionals, and lifelong learners</span>
                            explore new opportunities
                            <span class="green">and build</span>
                            successful careers through
                            <span class="yellow">trusted and up-to-date learning resources</span>.
                        </h3>
                    </div>
                </section>
            </div>          
            
        </div>

    </div>
</div>
<div class="container">
    <div class="row">
        <div class="col-md-12" style="text-align:center">            
            <div class="col-sm-12">
                <section style="text-align:center" id="module-anchor-statement-mission-statement" data-section-id="mission-statement" class="module-anchor-statement center module-anchor-statement__header-one">
                    <div>
                        <h3>
                            <span class="red">Our Vision</span>
                        </h3>
                        <h3>
                            Our vision is to
                            <span class="blue">become a globally recognized</span>
                            digital learning platform
                            <span class="red">that bridges the gap between education and employment</span>
                            — inspiring people to learn smarter, grow faster,
                            <span class="green">and create a meaningful impact</span>
                            in their
                            <span class="yellow">personal and professional lives</span>.
                        </h3>
                    </div>
                </section>
            </div>            

        </div>

    </div>
</div>
<div class="section-colored">

    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h3>Our Core Values</h3>
                @*<p>A complete website design featuring various single page templates from Start Bootstraps library of free HTML starter templates.</p>*@

            </div>
        </div>
        <div class="row" style="min-height:30px"></div>
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <ul style="list-style: none;padding-right:50px">
                    <li style="display:block;float:left;padding-top:3px"><picture> <img class="img-responsive" src="~/Images/about_5.png"></picture></li>
                    <li style="padding-left:30px;padding-bottom:15px" class="labelcss"><strong>Accessibility for All:</strong> We believe education is a universal right. Our goal is to make learning resources available to everyone, everywhere</li>
                    <li style="display:block;float:left;padding-top:3px"><picture> <img class="img-responsive" src="~/Images/about_5.png"></picture></li>
                    <li style="padding-left:30px;padding-bottom:15px" class="labelcss"><strong>Lifelong Learning:</strong> We encourage curiosity and continuous growth — adapting to new technologies and changing industry needs</li>
                    <li style="display:block;float:left;padding-top:3px"><picture> <img class="img-responsive" src="~/Images/about_5.png"></picture></li>
                    <li style="padding-left:30px;padding-bottom:15px" class="labelcss"><strong>Integrity & Authenticity:</strong> We stand by transparency and truth, ensuring every article, guide, and tutorial is reliable and trustworthy</li>
                    <li style="display:block;float:left;padding-top:3px"><picture> <img class="img-responsive" src="~/Images/about_5.png"></picture></li>
                    <li style="padding-left:30px;padding-bottom:15px" class="labelcss"><strong>Innovation in Education:</strong> We use technology and creativity to make learning smarter, more engaging, and results-oriented</li>
                    <li style="display:block;float:left;padding-top:3px"><picture> <img class="img-responsive" src="~/Images/about_5.png"></picture></li>
                    <li style="padding-left:30px;padding-bottom:15px" class="labelcss"><strong>Community Empowerment:</strong> We’re building a global learning community where ideas, knowledge, and collaboration help everyone grow together</li>
                </ul>
            </div>
            @*<div class="col-lg-5 col-md-5 col-sm-5">
                <img class="img-responsive" src="~/Images/About_11.png">
            </div>*@
        </div>
        <!-- /.row -->

    </div>
    <!-- /.container -->

</div>
<div class="clearfix"></div>
<div class="row" style="min-height:50px"></div>
