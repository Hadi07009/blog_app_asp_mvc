@model BootstrapERP.Models.SiteContentDetails
@{
    ViewBag.Title = "Portfolio";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="page-header">
                <h4 style="display:inline-block">@ViewBag.Title <small> </small></h4>
                <ol class="breadcrumbLine" style="float:right;display:inline-block">
                    <li>@Html.ActionLink("Home", "Index", "BlogPosts")</li>
                    <li class="active">@ViewBag.Title</li>
                </ol>
            </div>
        </div>
    </div>
</div>

@{
    var contentImageURLPart1 = Model.ContentDetailImageURL == null ? string.Empty : Model.ContentDetailImageURL;
}
@if (contentImageURLPart1 != string.Empty)
{
    <div id="myCarousel" class="carousel slide">
        <div class="carousel-inner">
            <div class="item active">
                <div class="fill" style="background-image:url('@Url.Content(contentImageURLPart1)');class=" img-responsive"">
                    <div class="container">
                        <div class="row">
                            <div class="col-sm-12">
                                <div class="row">
                                    <div class="form-group col-md-6" style="padding-top:15px">
                                        <h2>@Html.Raw(Model.ContentTitle)</h2>
                                        <h4>@Html.Raw(Model.ContentDetailSubTitle)</h4>
                                    </div>
                                    <div class="form-group col-md-6">

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}
<div class="container" style="min-height:100px;margin-top:15px">
    <div class="row">
        <div class="col-lg-9 col-md-9 col-sm-9">
            @Html.Raw(Model.ContentDescription)
        </div>
        <div class="col-lg-3 col-md-3 col-sm-3">
            <div >
                <img style="height: 200px; width: 100%;" class="img-responsive img-home-portfolio" src="~/Images/javascript.jpg">
            </div>
            <div>
                <img style="height: 200px; width: 100%;" class="img-responsive img-home-portfolio" src="~/Images/webapi.png">
            </div>
        </div>
    </div>
</div>

<div class="container" style="min-height:150px;margin-top:15px">
    <div class="row">
        <div class="col-lg-12 col-md-12 col-sm-12">
            
        </div>
    </div>
</div>




